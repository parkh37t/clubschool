{
  "name": "project-management-dashboard",
  "version": "1.0.0",
  "description": "프로젝트별 구성원 투입 현황을 관리하는 대시보드 애플리케이션",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "deploy": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build", 
    "deploy-check": "node deploy-check.js",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.487.0",
    "recharts": "^2.12.7",
    "react-hook-form": "^7.55.0",
    "sonner": "^2.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0-beta.5",
    "typescript": "~5.6.2",
    "vite": "^6.0.1"
  },
  "keywords": [
    "project-management",
    "dashboard",
    "react",
    "typescript",
    "tailwindcss",
    "clubschool"
  ],
  "author": "Clubschool Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/parkh37t/clubschool.git"
  },
  "homepage": "https://parkh37t.github.io/clubschool"
}